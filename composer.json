{
    "name": "orioap/vpic-vin-decoder",
    "description": "VIN Decoder for use with vPIC",
    "require": {
    	"laravel/framework": "5.*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
    	"laravel/laravel": "5.*",
    	"fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "authors": [
        {
            "name": "Ryan Andrews",
            "email": "ryan.andrews@orio.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
    	"psr-4": {
    		"VpicVinDecoder\\" : "src/"
    	}
    },
    "autoload-dev": {
        "psr-4": {
            "VpicVinDecoder\\Tests\\" : "tests"
        }
    }
}
