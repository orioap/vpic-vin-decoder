{
    "name": "orioap/vpic-vin-decoder",
    "description": "VIN Decoder for use with vPIC",
    "require": {
    	"laravel/framework": "5.*",
    	"illuminate/support": "5.*",
        "guzzlehttp/guzzle": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "authors": [
        {
            "name": "Ryan Andrews",
            "email": "ryan.andrews@orio.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
    	"psr-4": {
    		"VpicVinDecoder\\" : "src/"
    	}
    },
    "autoload-dev": {
        "psr-4": {
            "VpicVinDecoder\\Tests\\" : "tests"
        }
    }
}
